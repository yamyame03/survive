<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.user.dao.UserDAO">
 
  <select id="selectUser" resultType="kr.co.user.vo.UserVO">
  	select * from `user`;
  </select>
  
  <insert id="insertUser">
	insert into 
		`user` 
	set
		`nick` 		= #{nick},
		`number` 	= #{number},
		`reason` 	= #{reason},
		`regip` 	= #{regip},
		`username`	= #{username}; 
  </insert>
  
</mapper>