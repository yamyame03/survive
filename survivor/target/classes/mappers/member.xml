<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.survivor.dao.MemberDAO">
  
  <insert id="insertMember">
	insert into 
		`member` 
	set
		`username` 	= #{username},
		`password` 	= #{password},
		`email` 	= #{email},
		`hp` 		= #{hp},
		`level`		= 1,
		`regip` 	= #{regip}; 
  </insert>
  
</mapper>